  5048→      // Ripple effect on buttons\n  5049→      const rippleButtons = document.querySelectorAll('.btn-hero-primary, .btn-cta-wa, .btn-submit-rfq, .sticky-cta');\n  5050→      rippleButtons.forEach(function(button) {\n  5051→        button.classList.add('btn-ripple');\n  5052→      });\n  5053→\n  5054→      // Magnetic button effect (desktop only)\n  5055→      if (window.matchMedia('(hover: hover) and (pointer: fine)').matches) {\n  5056→        const magneticButtons = document.querySelectorAll('.btn-hero-primary, .btn-cta-wa');\n  5057→        \n  5058→        magneticButtons.forEach(function(button) {\n  5059→          button.addEventListener('mousemove', function(e) {\n  5060→            const rect = button.getBoundingClientRect();\n  5061→            const x = e.clientX - rect.left - rect.width / 2;\n  5062→            const y = e.clientY - rect.top - rect.height / 2;\n  5063→            \n  5064→            const moveX = x * 0.1;\n  5065→            const moveY = y * 0.1;\n  5066→            \n  5067→            button.style.transform = `translate(${moveX}px, ${moveY}px) translateY(-2px)`;\n  5068→          });\n  5069→          \n  5070→          button.addEventListener('mouseleave', function() {\n  5071→            button.style.transform = '';\n  5072→          });\n  5073→        });\n  5074→      }\n  5075→\n  5076→      // Parallax effect for hero section (subtle)\n  5077→      const heroSection = document.querySelector('.hero-modern');\n  5078→      if (heroSection && window.matchMedia('(prefers-reduced-motion: no-preference)').matches) {\n  5079→        window.addEventListener('scroll', function() {\n  5080→          const scrolled = window.scrollY;\n  5081→          const parallaxSpeed = 0.5;\n  5082→          \n  5083→          if (scrolled < window.innerHeight) {\n  5084→            heroSection.style.transform = `translateY(${scrolled * parallaxSpeed}px)`;\n  5085→          }\n  5086→        }, { passive: true });\n  5087→      }\n  5088→\n  5089→      // Animate numbers (count up effect)\n  5090→      function animateNumber(element, target, duration = 2000) {\n  5091→        const start = 0;\n  5092→        const increment = target / (duration / 16);\n  5093→        let current = start;\n  5094→        \n  5095→        const timer = setInterval(function() {\n  5096→      'use strict';\n  5097→\n  5098→          current += increment;\n  5099→          if (current >= target) {\n  5100→            element.textContent = target + '+';\n  5101→            clearInterval(timer);\n  5102→          } else {\n  5103→            element.textContent = Math.floor(current) + '+';\n  5104→          }\n  5105→        }, 16);\n  5106→      }\n  5107→\n  5108→      // Trigger number animation when visible\n  5109→      const trustTitle = document.querySelector('.trust-title');\n  5110→      if (trustTitle && trustTitle.textContent.includes('50+')) {\n  5111→        const observer = new IntersectionObserver(function(entries) {\n  5112→          entries.forEach(function(entry) {\n  5113→            if (entry.isIntersecting) {\n  5114→              // Animate the number\n  5115→              const numberSpan = document.createElement('span');\n  5116→              numberSpan.textContent = '0+';\n  5117→              trustTitle.innerHTML = trustTitle.innerHTML.replace('50+', '<span class=\"animated-number\">0+</span>');\n  5118→              \n  5119→              const animatedNumber = document.querySelector('.animated-number');\n  5120→              if (animatedNumber) {\n  5121→                animateNumber(animatedNumber, 50);\n  5122→              }\n  5123→              \n  5124→              observer.unobserve(entry.target);\n  5125→            }\n  5126→          });\n  5127→        }, { threshold: 0.5 });\n  5128→        \n  5129→        observer.observe(trustTitle);\n  5130→      }\n  5131→\n  5132→\n  5133→// ================ PERFORMANCE OPTIMIZATIONS ================\n  5134→      \n  5135→      // Enhanced lazy loading with intersection observer\n  5136→      const lazyImages = document.querySelectorAll('img[loading=\"lazy\"]');\n  5137→      \n  5138→      if ('IntersectionObserver' in window) {\n  5139→        const imageObserver = new IntersectionObserver(function(entries, observer) {\n  5140→          entries.forEach(function(entry) {\n  5141→            if (entry.isIntersecting) {\n  5142→              const img = entry.target;\n  5143→              \n  5144→              // Add loaded class when image loads\n  5145→              img.addEventListener('load', function() {\n  5146→                img.classList.add('loaded');\n  5147→              });\n  5148→              \n  5149→              // If image is already cached/loaded\n  5150→              if (img.complete) {\n  5151→                img.classList.add('loaded');\n  5152→              }\n  5153→              \n  5154→              observer.unobserve(img);\n  5155→            }\n  5156→          });\n  5157→        }, {\n  5158→          rootMargin: '50px 0px',\n  5159→          threshold: 0.01\n  5160→        });\n  5161→        \n  5162→        lazyImages.forEach(function(img) {\n  5163→          imageObserver.observe(img);\n  5164→        });\n  5165→      } else {\n  5166→        // Fallback for browsers without IntersectionObserver\n  5167→        lazyImages.forEach(function(img) {\n  5168→          img.classList.add('loaded');\n  5169→        });\n  5170→      }\n  5171→\n  5172→      // Preload critical images on hover (predictive loading)\n  5173→      const criticalLinks = document.querySelectorAll('a[href^=\"#\"]');\n  5174→      criticalLinks.forEach(function(link) {\n  5175→        link.addEventListener('mouseenter', function() {\n  5176→          const targetId = this.getAttribute('href').substring(1);\n  5177→          const targetSection = document.getElementById(targetId);\n  5178→          \n  5179→          if (targetSection) {\n  5180→            const images = targetSection.querySelectorAll('img[loading=\"lazy\"]');\n  5181→            images.forEach(function(img) {\n  5182→              if (!img.classList.contains('loaded')) {\n  5183→                // Trigger loading\n  5184→                const src = img.getAttribute('src');\n  5185→                if (src) {\n  5186→                  const preloadImg = new Image();\n  5187→                  preloadImg.src = src;\n  5188→                }\n  5189→              }\n  5190→            });\n  5191→          }\n  5192→        }, { once: true });\n  5193→      });\n  5194→\n  5195→      // Resource hints for external domains\n  5196→      function addResourceHint(url, rel) {\n  5197→        const link = document.createElement('link');\n  5198→        link.rel = rel;\n  5199→        link.href = url;\n  5200→        document.head.appendChild(link);\n  5201→      }\n  5202→\n  5203→      // Prefetch next likely pages\n  5204→      if ('requestIdleCallback' in window) {\n  5205→        requestIdleCallback(function() {\n  5206→      'use strict';\n  5207→\n  5208→          // Prefetch WhatsApp if user shows interest\n  5209→          const waLinks = document.querySelectorAll('a[href*=\"wa.me\"]');\n  5210→          waLinks.forEach(function(link) {\n  5211→            link.addEventListener('mouseenter', function() {\n  5212→              addResourceHint('https://wa.me', 'dns-prefetch');\n  5213→            }, { once: true });\n  5214→          });\n  5215→        });\n  5216→      }\n  5217→\n  5218→      // Defer non-critical CSS\n  5219→      const deferredStyles = document.querySelectorAll('link[media=\"print\"]');\n  5220→      deferredStyles.forEach(function(link) {\n  5221→        link.addEventListener('load', function() {\n  5222→          this.media = 'all';\n  5223→        });\n  5224→      });\n  5225→\n  5226→      // Monitor performance\n  5227→      if ('PerformanceObserver' in window) {\n  5228→        try {\n  5229→          const perfObserver = new PerformanceObserver(function(list) {\n  5230→            list.getEntries().forEach(function(entry) {\n  5231→              // Log slow resources (> 1s)\n  5232→              if (entry.duration > 1000) {\n  5233→                // Performance monitoring - disabled for production\n  5234→              }\n  5235→            });\n  5236→          });\n  5237→          \n  5238→          perfObserver.observe({ entryTypes: ['resource', 'navigation'] });\n  5239→        } catch (e) {\n  5240→          // Silently fail if not supported\n  5241→        }\n  5242→      }\n  5243→\n  5244→      // Service Worker registration (for future PWA support)\n  5245→      if ('serviceWorker' in navigator && location.protocol === 'https:') {\n  5246→        window.addEventListener('load', function() {\n  5247→          // Uncomment when service worker is ready\n  5248→          // navigator.serviceWorker.register('/sw.js').catch(function(err) {\n  5249→          //   // console.log('SW registration failed:', err); // Disabled for production\n  5250→          // });\n  5251→        });\n  5252→      }\n  5253→\n  5254→\n  5255→// ================ FORM VALIDATION & ENHANCEMENTS ================\n  5256→      \n  5257→      // Real-time validation functions\n  5258→      const validators = {\n  5259→        name: function(value) {\n  5260→          if (!value || value.trim().length < 2) {\n  5261→            return { valid: false, message: 'Nama minimal 2 karakter' };\n  5262→          }\n  5263→          return { valid: true, message: 'Nama valid' };\n  5264→        },\n  5265→        \n  5266→        whatsapp: function(value) {\n  5267→          // Indonesian phone format: 08xx-xxxx-xxxx\n  5268→          const cleaned = value.replace(/\\D/g, '');\n  5269→          if (!cleaned.startsWith('08')) {\n  5270→            return { valid: false, message: 'Nomor harus diawali 08' };\n  5271→          }\n  5272→          if (cleaned.length < 10 || cleaned.length > 13) {\n  5273→            return { valid: false, message: 'Nomor tidak valid 