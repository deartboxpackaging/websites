al-time validation for all fields\n  5323→      function validateField(field, validatorName) {\n  5324→        const fieldWrapper = field.closest('.form-field');\n  5325→        if (!fieldWrapper) return;\n  5326→        \n  5327→        const value = field.value.trim();\n  5328→        const validator = validators[validatorName];\n  5329→        \n  5330→        if (!validator) return;\n  5331→        \n  5332→        const result = validator(value);\n  5333→        \n  5334→        // Remove existing error/success messages\n  5335→        let errorMsg = fieldWrapper.querySelector('.form-error');\n  5336→        let successMsg = fieldWrapper.querySelector('.form-success');\n  5337→        \n  5338→        if (!errorMsg) {\n  5339→          errorMsg = document.createElement('div');\n  5340→          errorMsg.className = 'form-error';\n  5341→          field.parentNode.appendChild(errorMsg);\n  5342→        }\n  5343→        \n  5344→        if (!successMsg) {\n  5345→          successMsg = document.createElement('div');\n  5346→          successMsg.className = 'form-success';\n  5347→          field.parentNode.appendChild(successMsg);\n  5348→        }\n  5349→        \n  5350→        if (result.valid) {\n  5351→          fieldWrapper.classList.remove('error');\n  5352→          fieldWrapper.classList.add('valid');\n  5353→          errorMsg.textContent = '';\n  5354→          successMsg.textContent = result.message;\n  5355→        } else {\n  5356→          fieldWrapper.classList.remove('valid');\n  5357→          fieldWrapper.classList.add('error');\n  5358→          errorMsg.textContent = result.message;\n  5359→          successMsg.textContent = '';\n  5360→        }\n  5361→      }\n  5362→\n  5363→      // Attach validators to form fields\n  5364→      const nameInput = document.getElementById('rfq-name');\n  5365→      const emailInput = document.getElementById('rfq-email');\n  5366→      \n  5367→      if (nameInput) {\n  5368→        nameInput.addEventListener('blur', function() {\n  5369→          validateField(this, 'name');\n  5370→        });\n  5371→      }\n  5372→      \n  5373→      if (emailInput) {\n  5374→        emailInput.addEventListener('blur', function() {\n  5375→          validateField(this, 'email');\n  5376→        });\n  5377→      }\n  5378→\n  5379→      // ================ FILE UPLOAD HANDLING ================\n  5380→      \n  5381→      const fileInput = document.getElementById('rfq-files');\n  5382→      const fileLabel = document.querySelector('.file-upload-label');\n  5383→      const filePreview = document.getElementById('filePreview');\n  5384→      let uploadedFiles = [];\n  5385→\n  5386→      if (fileInput && fileLabel) {\n  5387→        // Click to upload\n  5388→        fileInput.addEventListener('change', handleFiles);\n  5389→        \n  5390→        // Drag and drop\n  5391→        fileLabel.addEventListener('dragover', function(e) {\n  5392→          e.preventDefault();\n  5393→          this.classList.add('dragover');\n  5394→        });\n  5395→        \n  5396→        fileLabel.addEventListener('dragleave', function(e) {\n  5397→          e.preventDefault();\n  5398→          this.classList.remove('dragover');\n  5399→        });\n  5400→        \n  5401→        fileLabel.addEventListener('drop', function(e) {\n  5402→          e.preventDefault();\n  5403→          this.classList.remove('dragover');\n  5404→          \n  5405→          const files = e.dataTransfer.files;\n  5406→          handleFiles({ target: { files: files } });\n  5407→        });\n  5408→      }\n  5409→\n  5410→      function handleFiles(e) {\n  5411→        const files = Array.from(e.target.files);\n  5412→        const maxSize = 5 * 1024 * 1024; // 5MB\n  5413→        const allowedTypes = ['image/jpeg', 'image/png', 'image/webp', 'application/pdf'];\n  5414→        \n  5415→        files.forEach(file => {\n  5416→          // Validate file size\n  5417→          if (file.size > maxSize) {\n  5418→    